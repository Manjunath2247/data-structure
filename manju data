#assigning elements in list
list = [1,2,3,4,5]
list2 = [6,7,8]

print(list+list2)

#accessing elements from tuple
number=('m','n','b','v')

print(number[1])
print(number[2])


#deleting different dictionary elements

dict = {1:2,
        3:4,
        5:6,
        7:8}

del dict[1]
del dict[5]
print(dict)
